<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <header>
        <section>
            <h1>
                <a class="img-troca" href="~/" title="Translado">Translado</a>
            </h1>
            <ul class="menu">
                <li>
                    @Html.ActionLink("Usuarios", "Index", "Usuario")
                </li>
                <li>
                    @Html.ActionLink("Motoristas", "Index", "Motorista")
                </li>
                <li>
                    @Html.ActionLink("Veiculos", "Index", "Veiculo")
                </li>
                <li>
                    @Html.ActionLink("Passageiros", "Index", "Passageiro")
                </li>
                <li>
                    @Html.ActionLink("Agenda", "Index", "Agenda")
                </li>
                <li>
                    @Html.ActionLink("Companhia", "Index", "Companhia")
                </li>
            </ul>
        </section>
    </header>
    @RenderBody()
    <footer>
        <div>

        </div>
    </footer>
    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
</body>
</html>
